function CheckKey(key)
    local keyValid = http_req({
      Url = "https://bstlar.com/keys/validate/"..key,
      Method = "GET",
      Headers = {
        ["bstk"] = "f3eZ5Rldvkv7b89YG3xO8fnpOVM6x0vYU0IA" --Your boostellar header here.
      },
    })
  
    return game:GetService("HttpService"):JSONDecode(keyValid.Body).valid
  end
  
  
  local DiscordLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/NUTellaVING/Pastedrop/main/Library.lua"))()
  local win = DiscordLib:Window("Central Hub (10+ Games/100+ Scripts)") -- Change the TOP ui name.
  local serv = win:Server("Home","rbxassetid://14188671658") -- Change the Windows ui name.
  local Kys = serv:Channel("Key System") -- Change the first tab name.
  
  Kys:Textbox("Key Box", "Put the key here!", false, function(key)
      print("Input Key: "..key)
      local isValid = CheckKey(key)
      if isValid then
          game.CoreGui.Discord:Destroy()
          beeeeeeeee()
          writefile("Central.txt", key)
      else
      end
  end)
  
  Kys:Button("Get Key Link!", function()
      DiscordLib:Notification("Notification", "Key Link has been copied to clipboard.", "Okay!")
      setclipboard("https://central-script.com/getkey")
  end)
  Kys:Label("central-script.com/getkey")
  
  local valid = isfile("Central.txt") 
  if valid == true then
      local key = readfile("Central.txt")
      print("Input Key: "..key)
      local isValid = CheckKey(key)
      if isValid then
        game.CoreGui.Discord:Destroy()
            beeeeeeeee()
          writefile("Central.txt", key)
      else
      end
  end




