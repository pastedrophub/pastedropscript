local function generateDailyKey(seed)
    local chars = 'ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789'
    local key = ''
    for i = 1, 10 do
        local index = ((string.byte(seed, i) + string.byte(seed, i + 1)) % #chars) + 1
        key = key .. chars:sub(index, index)
    end
    return key
end

local function CheckKey(key)

    local currentDate = os.date("%Y-%m-%d")
    local seed = currentDate .. "your secret phrase here"
    local generatedKey = generateDailyKey(seed)

    return key == generatedKey
end



local DiscordLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/pastedrophub/pastedropscript/main/Library.lua"))()
local win = DiscordLib:Window("Paste-Drop Hub | 100+ Scripts") -- Change the TOP ui name.
local serv = win:Server("Home", "rbxassetid://99523336823193")
local Kys = serv:Channel("Key System")

Kys:Textbox("Key Box", "Put the key here!", false, function(key)
    print("Input Key: " .. key)
    local isValid = CheckKey(key)
    if isValid then
        game.CoreGui.Discord:Destroy()
        beeeeeeeee()
    else
        DiscordLib:Notification("Invalid Key", "The key you entered is not valid.", "Okay!")
    end
end)

Kys:Label("Hit ENTER once you've entered your key.")


Kys:Button("Get Key Link!", function()
    DiscordLib:Notification("Notification", "Key Link has been copied to clipboard.", "Okay!")
    setclipboard("https://paste-drophub.com/getkey/")
end)

Kys:Label("paste-drophub.com/getkey/")
